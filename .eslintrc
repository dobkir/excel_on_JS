{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "max-len": ["error", {
            "code": 120
        }],
        "object-curly-spacing": ["error", "always"]
    },
    "extends": [
        "eslint:recommended", "google"
    ]
}